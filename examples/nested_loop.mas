
LDI 2, 10    ; R2 = 10

LDI 0, 0    ; R0 = 0

LBL loop1 ; first loop
LDI 1, 0    ; R1 = 0

LBL loop2 ; second loop
    ADDI 1, 1, 1  ; R1++
    PUTS 4, 'R', '1', ':', ' '
    PUTN 1
    FLUSH
    CEQ 1, 2      ; R1 == 10?
    JF loop2      ; if not, jump back to loop 2

ADDI 0, 1, 0  ; R0++
FLUSH
PUTS 4, 'R', '0', ':', ' '
PUTN 0
FLUSH
FLUSH
CEQ 0, 2      ; R0 == 10?
JF loop1      ; if not, jump back to loop 1
